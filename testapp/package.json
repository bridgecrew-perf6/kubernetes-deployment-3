{
  "name": "nextjs-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run nextDev\" \"npm run wundergraph\" \"npm run browser\"",
    "wundergraph": "cd .wundergraph && wunderctl up --debug",
    "browser": "wait-on \"http-get://localhost:3000\" && wait-on \"http-get://localhost:9991\" && open-cli http://localhost:3000",
    "build": "next build",
    "nextDev": "next dev",
    "start": "next start",
    "wunderctl": "wunderctl"
  },
  "dependencies": {
    "@wundergraph/sdk": "^1.0.0-next.17",
    "next": "^12.0.10",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@types/node": "^17.0.15",
    "@types/react": "^17.0.3",
    "concurrently": "^6.0.0",
    "open-cli": "^7.0.1",
    "typescript": "^4.3.5",
    "wait-on": "^6.0.0"
  }
}
